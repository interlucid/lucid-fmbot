{
  "name": "lucid-fmbot",
  "version": "0.1.0",
  "description": "a Discord.js bot that implements some missing features of .fmbot",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build:watch": "nodemon --config nodemon-build.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "register-commands": "node dist/register-commands.cjs",
    "start": "node --enable-source-maps dist/main.cjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -b",
    "watch": "nodemon --config nodemon-app.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interlucid/lucid-fmbot.git"
  },
  "keywords": [
    "interlucid"
  ],
  "author": "Brandon der Bl√§tter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/interlucid/lucid-fmbot/issues"
  },
  "homepage": "https://github.com/interlucid/lucid-fmbot#readme",
  "dependencies": {
    "croner": "^6.0.3",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "lastfm": "^0.9.4",
    "luxon": "^3.3.0",
    "mongodb": "^5.5.0",
    "nodemon": "^2.0.22"
  },
  "devDependencies": {
    "@swc/core": "^1.3.60",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
